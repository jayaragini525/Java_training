import java.util.*;
import java.lang.String;
class HelloWorld {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String str="Hello";
   
        for(int i=0;i<str.length();i++){
           System.out.println(str.charAt(i));
           if(i<str.length()-1)
           System.out.println(1);
        }
    }
}
