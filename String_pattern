import java.util.*;
import java.lang.String;
class HelloWorld {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        System.out.println("enter the String:");
        String str=s.nextLine();
        for(int i=0;i<str.length();i++){
            for(int j=0;j<=i;++j){
                char ch=str.charAt(j);
                System.out.print(ch+" ");
            }
            System.out.println();
            }
        }
    }
